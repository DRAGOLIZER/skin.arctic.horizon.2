<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="View_Furniture_Wall_Bottom">
        <control type="group">
            <visible>$EXP[Exp_WallViews]</visible>
            <!-- View -->
            <include content="Object_Control">
                <param name="control" value="group" />
                <include content="List_Square_Wall">
                    <param name="id" value="507" />
                    <param name="itemlayout_h" value="240" />
                    <param name="item_h" value="200" />
                    <param name="itemlayout_include" value="Layout_Button" />
                    <param name="detailed" value="true" />
                    <bottom>1</bottom>
                </include>
                <!-- <include>View_507_Square_Vert_Combined</include> -->
                <!-- <include>View_520_Poster_Wall</include> -->
            </include>
        </control>

    </include>

    <include name="View_507_Square_Vert_Combined">
        <control type="group">
            <visible>Control.IsVisible(507)</visible>
            <include content="List_Button_Row">
                <param name="id" value="507" />
                <param name="offset_y" value="40" />
                <param name="movement" value="5" />
                <param name="orientation" value="vertical" />
                <param name="controltype" value="fixedlist" />
                <param name="item_w" value="560" />
                <param name="itemlayout_w" value="600" />
                <param name="itemlayout_h" value="144" />
                <param name="item_h" value="104" />
                <param name="itemlayout_include" value="Layout_IconButton" />
                <param name="altfocus" value="true" />
                <param name="detailed" value="true" />
                <top>160</top>
                <bottom>160</bottom>
                <preloaditems>0</preloaditems>
                <onright>508</onright>
            </include>
            <include content="List_Square_Wall">
                <param name="id" value="508" />
                <onleft>507</onleft>
                <left>600</left>
                <bottom>1</bottom>
                <visible>Control.IsVisible(507)</visible>
                <visible>Container(507).ListItem.IsFolder</visible>
                <content target="videos">$INFO[Container(507).ListItem.FolderPath]</content>
            </include>
            <include content="Object_WallFade">
                <param name="id" value="508" />
                <param name="altvisible_top" value="true" />
                <param name="altvisible_bottom" value="true" />
            </include>
        </control>
    </include>

    <include name="View_520_Poster_Wall">
        <control type="group">
            <visible>Control.IsVisible(520)</visible>
            <!-- <include>Dimension_View</include> -->
            <include content="List_Poster_Wall">
                <param name="id" value="520" />
                <!-- <param name="offset_y" value="40" />
                <left>-80</left>
                <top>-40</top>
                <right>474</right>
                <bottom>-40</bottom>
                <preloaditems>0</preloaditems> -->
                <bottom>1</bottom>
                <pagecontrol>60</pagecontrol>
                <onleft>60</onleft>
            </include>
            <!-- <include content="Object_BigPoster">
                <right>0</right>
                <width>474</width>
            </include> -->
            <include content="Object_WallFade">
                <param name="id" value="520" />
            </include>
        </control>
    </include>

</includes>