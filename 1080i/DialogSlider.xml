<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">11</defaultcontrol>
    <controls>
        <include content="Dialog_Background">
            <include>Animation_SlideIn_Dialog</include>
            <bottom>200</bottom>
            <left>view_pad</left>
            <right>view_pad</right>
            <height>120</height>
            <include content="Dialog_Header">
                <param name="id" value="10" />
                <align>left</align>
            </include>
            <include content="Dialog_Header">
                <param name="id" value="12" />
                <align>right</align>
            </include>
            <control type="group">
                <height>12</height>
                <centertop>50%</centertop>
                <left>view_pad</left>
                <right>view_pad</right>
                <!-- Left Side Marker -->
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerleft>12</centerleft>
                    <top>-8</top>
                </control>
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerleft>12</centerleft>
                    <bottom>-8</bottom>
                </control>
                <!-- Left Center Marker -->
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerleft>25%</centerleft>
                    <top>-8</top>
                </control>
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerleft>25%</centerleft>
                    <bottom>-8</bottom>
                </control>
                <!-- Center Marker -->
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>12</height>
                    <centerleft>50%</centerleft>
                    <top>-12</top>
                </control>
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>12</height>
                    <centerleft>50%</centerleft>
                    <bottom>-12</bottom>
                </control>
                <!-- Right Center Marker -->
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerright>25%</centerright>
                    <top>-8</top>
                </control>
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerright>25%</centerright>
                    <bottom>-8</bottom>
                </control>
                <!-- Right Side Marker -->
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerright>12</centerright>
                    <top>-8</top>
                </control>
                <control type="image">
                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                    <width>4</width>
                    <height>8</height>
                    <centerright>12</centerright>
                    <bottom>-8</bottom>
                </control>
                <control type="slider" id="11">
                    <textureslidernibfocus colordiffuse="$VAR[ColorHighlight]" border="6">slider/slider_nib.png</textureslidernibfocus>
                    <ondown>Close</ondown>
                    <onup>Close</onup>
                </control>
            </control>
        </include>

    </controls>
</window>
