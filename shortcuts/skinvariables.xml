        <value condition="!String.IsEmpty({listitemposition}.Title)">$INFO[{listitemposition}.Title]</value>
        <value condition="!String.IsEmpty({listitemposition}.Label)">$INFO[{listitemposition}.Label]</value>
        <!-- <value condition="!String.IsEmpty({listitemposition}.Label) + [!String.IsEmpty({listitemposition}.DBType) | !String.IsEmpty({listitemposition}.ChannelNumberLabel) | !String.IsEmpty({listitemposition}.ChannelName) | !String.IsEmpty({listitemposition}.AddonVersion)]">$INFO[{listitemposition}.Label]</value> -->
